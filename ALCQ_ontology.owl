<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xml:base="http://alcq.org/onto.owl"
         xmlns="http://alcq.org/onto.owl#">

<owl:Ontology rdf:about="http://alcq.org/onto.owl"/>

<owl:ObjectProperty rdf:about="#mentors">
  <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  <rdfs:range rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#admires">
  <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  <rdfs:range rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#supervises">
  <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  <rdfs:range rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#instructs">
  <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  <rdfs:range rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#guides">
  <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  <rdfs:range rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#leads">
  <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  <rdfs:range rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
</owl:ObjectProperty>

<owl:Class rdf:about="#pos_innovative">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
</owl:Class>

<owl:Class rdf:about="#pos_determined">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  <rdfs:subClassOf rdf:resource="#only_supervises_d_L_only_mentors_d_L_pos_innovative_R_R"/>
</owl:Class>

<owl:Class rdf:about="#neg_innovative">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  <owl:equivalentClass>
    <owl:Class>
      <owl:complementOf rdf:resource="#pos_innovative"/>
    </owl:Class>
  </owl:equivalentClass>
</owl:Class>

<owl:Class rdf:about="#pos_ambitious">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
</owl:Class>

<owl:Class rdf:about="#neg_ambitious">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  <owl:equivalentClass>
    <owl:Class>
      <owl:complementOf rdf:resource="#pos_ambitious"/>
    </owl:Class>
  </owl:equivalentClass>
</owl:Class>

<owl:Class rdf:about="#pos_confident">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
</owl:Class>

<owl:Class rdf:about="#neg_confident">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  <owl:equivalentClass>
    <owl:Class>
      <owl:complementOf rdf:resource="#pos_confident"/>
    </owl:Class>
  </owl:equivalentClass>
</owl:Class>

<owl:Class rdf:about="#only_leads_d_L_pos_determined_R">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  <owl:equivalentClass>
    <owl:Restriction>
      <owl:onProperty rdf:resource="#leads"/>
      <owl:allValuesFrom rdf:resource="#pos_determined"/>
    </owl:Restriction>
  </owl:equivalentClass>
  <rdfs:subClassOf rdf:resource="#pos_determined"/>
</owl:Class>

<owl:Class rdf:about="#exists_admires_d_L_pos_confident_R">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  <owl:equivalentClass>
    <owl:Restriction>
      <owl:onProperty rdf:resource="#admires"/>
      <owl:someValuesFrom rdf:resource="#pos_confident"/>
    </owl:Restriction>
  </owl:equivalentClass>
</owl:Class>

<owl:Class rdf:about="#exists_guides_d_L_neg_ambitious_R">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  <owl:equivalentClass>
    <owl:Restriction>
      <owl:onProperty rdf:resource="#guides"/>
      <owl:someValuesFrom rdf:resource="#neg_ambitious"/>
    </owl:Restriction>
  </owl:equivalentClass>
  <rdfs:subClassOf rdf:resource="#only_mentors_d_L_L_pos_determined_R_or_L_AM3_instructs_d_L_neg_confident_R_R_R"/>
</owl:Class>

<owl:Class rdf:about="#AM3_instructs_d_L_neg_confident_R">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  <owl:equivalentClass>
    <owl:Restriction>
      <owl:onProperty rdf:resource="#instructs"/>
      <owl:onClass rdf:resource="#neg_confident"/>
      <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">3</owl:maxQualifiedCardinality>
    </owl:Restriction>
  </owl:equivalentClass>
</owl:Class>

<owl:Class rdf:about="#only_mentors_d_L_pos_innovative_R">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  <owl:equivalentClass>
    <owl:Restriction>
      <owl:onProperty rdf:resource="#mentors"/>
      <owl:allValuesFrom rdf:resource="#pos_innovative"/>
    </owl:Restriction>
  </owl:equivalentClass>
</owl:Class>

<owl:Class rdf:about="#only_supervises_d_L_only_mentors_d_L_pos_innovative_R_R">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  <owl:equivalentClass>
    <owl:Restriction>
      <owl:onProperty rdf:resource="#supervises"/>
      <owl:allValuesFrom rdf:resource="#only_mentors_d_L_pos_innovative_R"/>
    </owl:Restriction>
  </owl:equivalentClass>
</owl:Class>

<owl:Class rdf:about="#L_pos_determined_R_or_L_AM3_instructs_d_L_neg_confident_R_R">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  <owl:equivalentClass>
    <owl:Class>
      <owl:unionOf rdf:parseType="Collection">
        <rdf:Description rdf:about="#pos_determined"/>
        <rdf:Description rdf:about="#AM3_instructs_d_L_neg_confident_R"/>
      </owl:unionOf>
    </owl:Class>
  </owl:equivalentClass>
</owl:Class>

<owl:Class rdf:about="#only_mentors_d_L_L_pos_determined_R_or_L_AM3_instructs_d_L_neg_confident_R_R_R">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  <owl:equivalentClass>
    <owl:Restriction>
      <owl:onProperty rdf:resource="#mentors"/>
      <owl:allValuesFrom rdf:resource="#L_pos_determined_R_or_L_AM3_instructs_d_L_neg_confident_R_R"/>
    </owl:Restriction>
  </owl:equivalentClass>
  <rdfs:subClassOf rdf:resource="#only_leads_d_L_pos_determined_R"/>
</owl:Class>

<owl:Thing rdf:about="#Manolis">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
</owl:Thing>

<owl:Thing rdf:about="#Panos">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
  <rdf:type rdf:resource="#exists_guides_d_L_neg_ambitious_R"/>
  <supervises rdf:resource="#Maria"/>
  <admires rdf:resource="#Manolis"/>
  <rdf:type rdf:resource="#neg_innovative"/>
</owl:Thing>

<owl:Thing rdf:about="#Maria">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
</owl:Thing>

<owl:Thing rdf:about="#Dimitrios">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
  <rdf:type rdf:resource="#exists_admires_d_L_pos_confident_R"/>
</owl:Thing>

<owl:AllDifferent>
  <owl:distinctMembers rdf:parseType="Collection">
    <rdf:Description rdf:about="#Manolis"/>
    <rdf:Description rdf:about="#Panos"/>
    <rdf:Description rdf:about="#Maria"/>
    <rdf:Description rdf:about="#Dimitrios"/>
  </owl:distinctMembers>
</owl:AllDifferent>


</rdf:RDF>
